    //36println("circle:" + circle(readLine()!!.toInt()))
    //println("tc")
    //println("fahrenheit" + fahrenheit(readLine()!!.toDouble()))
    //println(abxy(120.5, 130.7, 8.2, 4.4))
    //println(String.format("%.2f",PI))// "pi = %.${2}f" + formatPI

   println("Byte: ")
   println("byte file "+ integer3(readLine()!!.toDouble()))

}
